{
  "name": "cdn-logs-collector",
  "version": "1.0.0",
  "description": "Transform G-Core CDN logs from S3 to OpenTelemetry and send to Grafana Alloy (TypeScript)",
  "type": "module",
  "main": "dist/collector.js",
  "bin": {
    "collector": "dist/collector.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/collector.js",
    "collector": "node dist/collector.js",
    "verify": "node dist/collector.js --verify"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/credential-providers": "^3.700.0",
    "@opentelemetry/api-logs": "^0.52.0",
    "@opentelemetry/exporter-logs-otlp-grpc": "^0.52.0",
    "@opentelemetry/sdk-logs": "^0.52.0",
    "@opentelemetry/resources": "^1.28.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.6.0"
  }
}
